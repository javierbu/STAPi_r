  <source srcset="https://github.githubassets.com/images/modules/site/issues/custom-fields.png?width=1416&amp;format=webpll 1416w,https://github.githubassets.com/images/modules/site/issues/custom-fields.png?width=1132&amp;format=webpll 1132w,https://github.githubassets.com/images/modules/site/issues/custom-fields.png?width=849&amp;format=webpll 849w,https://github.githubassets.com/images/modules/site/issues/custom-fields.png?width=708&amp;format=webpll 708w,https://github.githubassets.com/images/modules/site/issues/custom-fields.png?width=566&amp;format=webpll 566w,https://github.githubassets.com/images/modules/site/issues/custom-fields.png?width=283&amp;format=webpll 283w" sizes="(max-width: 768px) 90vw, (max-width: 1320px) 45vw, 616px" type="image/webp"></source>
